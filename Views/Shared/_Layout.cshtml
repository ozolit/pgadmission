                            <a class="nav-link text-dark glyphicon glyphicon-user" asp-area="" asp-controller="Home" asp-action="Index"><i class="bi bi-box-arrow-in-right"></i>Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark glyphicon glyphicon-log-in" asp-area="" asp-controller="Home" asp-action="Privacy"><i class="bi bi-4-square"></i>Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>

